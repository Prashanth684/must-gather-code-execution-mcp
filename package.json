{
  "name": "must-gather-mcp-server",
  "version": "2.0.0",
  "description": "Progressive Disclosure MCP server for efficient OpenShift must-gather analysis",
  "type": "module",
  "main": "dist/must-gather-lib.js",
  "bin": {
    "must-gather-server": "dist/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/mcp-server.js",
    "dev": "tsx mcp-server.ts",
    "example:failing-pods": "tsx examples/analysis-patterns.ts failing-pods",
    "example:health": "tsx examples/analysis-patterns.ts health-overview",
    "example:correlate": "tsx examples/analysis-patterns.ts correlate-events",
    "example:namespaces": "tsx examples/analysis-patterns.ts namespace-analysis",
    "example:detect": "tsx examples/analysis-patterns.ts detect-issues",
    "example:mcp": "tsx examples/mcp-usage-example.ts",
    "example:prodisco": "tsx examples/progressive-disclosure-demo.ts"
  },
  "keywords": [
    "openshift",
    "must-gather",
    "mcp",
    "kubernetes",
    "diagnostics",
    "progressive-disclosure",
    "code-execution"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
